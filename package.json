    "prosemirror-commands": "^1.5.2",
    "prosemirror-state": "^1.4.3",
    "quibble": "^0.9.2",
    "rxjs": "^7.8.1",
    "sinon": "^18.0.0",
    "tslib": "^2.6.3",
    "uuid": "^10.0.0",
    "zone.js": "^0.14.8"
