{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "analytics": false,
    "packageManager": "npm",
    "schematicCollections": ["@angular-eslint/schematics"],
    "cache": { "environment": "all" }
  },
  "newProjectRoot": "projects",
  "projects": {
    "jz-portfolio": {
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "configurations": {
            "production": {
              "outputPath": {
                "base": "dist/jz-portfolio",
                "browser": ""
              },
              "index": {
                "input": "src/index.prod.html",
                "output": "index.html"
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "10mb"
                }
              ],
              "statsJson": false,
              "subresourceIntegrity": true
            },
            "development": {
              "outputPath": {
                "base": "dist/jz-portfolio-dev",
                "browser": ""
              },
              "index": {
                "input": "src/index.html",
                "output": "index.html"
              },
              "optimization": true,
              "statsJson": false
            },
            "dev-local": {
              "outputPath": {
                "base": "dist/jz-portfolio-dev-local",
                "browser": ""
              },
              "index": {
                "input": "src/index.html",
                "output": "index.html"
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev-local.ts"
                }
              ],
              "verbose": true
            },
            "local": {
              "outputPath": {
                "base": "dist/jz-portfolio-local",
                "browser": ""
              },
              "index": {
                "input": "src/index.html",
                "output": "index.html"
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.local.ts"
                }
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10kb",
                  "maximumError": "15mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "5kb",
                  "maximumError": "15mb"
                }
              ],

              "serviceWorker": false,
              "verbose": true
            }
          },
          "options": {
            "allowedCommonJsDependencies": [
              "firebase",
              "@angular/fire",
              "@firebase/storage",
              "@firebase/component"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "budgets": [
              {
                "type": "initial",
                "maximumWarning": "10mb",
                "maximumError": "15mb"
              },
              {
                "type": "anyComponentStyle",
                "maximumWarning": "5kb",
                "maximumError": "15mb"
              }
            ],
            "extractLicenses": false,
            "index": "src/index.html",
            "namedChunks": true,
            "optimization": false,
            "polyfills": ["zone.js", "@angular/localize/init"],
            "scripts": [],
            "serviceWorker": "ngsw-config.json",
            "sourceMap": true,
            "styles": ["src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["src/style_vars"]
            },
            "tsConfig": "tsconfig.app.json",
            "browser": "src/main.ts",
            "preserveSymlinks": true,
            "outputHashing": "all",
            "subresourceIntegrity": false,
            "statsJson": true
          },
          "defaultConfiguration": "development"
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "configurations": {
            "production": {
              "index": "src/index.prod.html",
              "browserTarget": "jz-portfolio:build:production",
              "firebaseProject": "jz-portfolio",
              "firebaseHostingSite": "jlz-portfolio"
            },
            "development": {
              "index": "src/index.html",
              "browserTarget": "jz-portfolio:build:development",
              "firebaseProject": "jz-portfolio",
              "firebaseHostingSite": "jlz-portfolio-dev"
            }
          },
          "options": {
            "browserTarget": "jz-portfolio:build",
            "firebaseHostingSite": "jlz-portfolio-dev",
            "firebaseProject": "jz-portfolio",
            "prerender": true,
            "ssr": false
          }
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n",
          "options": {
            "buildTarget": "jz-portfolio:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "jz-portfolio:build:production"
            },
            "development": {
              "buildTarget": "jz-portfolio:build:development"
            },
            "dev-local": {
              "buildTarget": "jz-portfolio:build:dev-local"
            },
            "local": {
              "buildTarget": "jz-portfolio:build:local"
            }
          },
          "options": {
            "buildTarget": "jz-portfolio:build"
          },
          "defaultConfiguration": "local"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "fileReplacements": [
              {
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.test.ts"
              }
            ],
            "codeCoverage": true,
            "polyfills": [
              "zone.js",
              "zone.js/testing",
              "@angular/localize/init"
            ],
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "styles": ["src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["src/style_vars"]
            },
            "scripts": [],
            "preserveSymlinks": true
          }
        },
        "e2e": {
          "builder": "@puppeteer/ng-schematics:puppeteer",
          "options": {
            "devServerTarget": "jz-portfolio:serve",
            "testRunner": "mocha"
          },
          "configurations": {
            "local": {
              "devServerTarget": "jz-portfolio:serve:local"
            }
          }
        }
      },
      "prefix": "jzp",
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        },
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "src"
    }
  },
  "schematics": {
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    },
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
