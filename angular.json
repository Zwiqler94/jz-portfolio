{
  "version": 1,
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false,
    "packageManager": "npm",
    "schematicCollections": ["@angular-eslint/schematics"]
  },
  "newProjectRoot": "projects",
  "projects": {
    "jz-portfolio": {
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "configurations": {
            "production": {
              "outputPath": "dist/jz-portfolio",
              "index": {
                "input": "src/index.prod.html",
                "output": "index.html"
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "bundles",
              "sourceMap": {
                "scripts": true,
                "styles": true
              },
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "aot": true,
              "commonChunk": true,
              "serviceWorker": true,
              "statsJson": true,
              "subresourceIntegrity": true,
              "progress": true
            },
            "development": {
              "outputPath": "dist/jz-portfolio-dev",
              "index": {
                "input": "src/index.html",
                "output": "index.html"
              },
              "optimization": false,
              "sourceMap": true,
              "namedChunks": true,
              "vendorChunk": true,
              "buildOptimizer": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "12mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "10kb"
                }
              ],
              "aot": true,
              "commonChunk": false,
              "serviceWorker": true,
              "statsJson": true,
              "subresourceIntegrity": true,
              "progress": true
            },
            "local": {
              "outputPath": "dist/jz-portfolio-dev-local",
              "index": {
                "input": "src/index.html",
                "output": "index.html"
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.local.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "bundles",
              "sourceMap": true,
              "namedChunks": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "15mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "5kb",
                  "maximumError": "10kb"
                }
              ],
              "aot": true,
              "commonChunk": true,
              "serviceWorker": true,
              "statsJson": true,
              "subresourceIntegrity": true,
              "progress": true
            }
          },
          "options": {
            "customWebpackConfig": {
              "path": "./webpack.config.ts"
            },
            "allowedCommonJsDependencies": [
              "firebase",
              "@angular/fire",
              "@firebase/storage",
              "@firebase/component"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "buildOptimizer": true,
            "extractLicenses": true,
            "index": "src/index.html",
            "main": "src/main.ts",
            "namedChunks": true,
            "ngswConfigPath": "ngsw-config.json",
            "optimization": false,
            "polyfills": "src/polyfills.ts",
            "scripts": [],
            "serviceWorker": true,
            "sourceMap": true,
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
              "src/styles.scss"
            ],
            "tsConfig": "tsconfig.app.json",
            "vendorChunk": true
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "configurations": {
            "production": {
              "index": "src/index.prod.html",
              "browserTarget": "jz-portfolio:build:production",
              "firebaseProject": "jz-portfolio",
              "firebaseHostingSite": "jlz-portfolio"
            },
            "development": {
              "index": "src/index.html",
              "browserTarget": "jz-portfolio:build:development",
              "firebaseProject": "jz-portfolio",
              "firebaseHostingSite": "jlz-portfolio-dev"
            }
          },
          "options": {
            "browserTarget": "jz-portfolio:build",
            "firebaseHostingSite": "jlz-portfolio-dev",
            "firebaseProject": "jz-portfolio",
            "prerender": true,
            "ssr": false
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "configurations": {
            "production": {
              "devServerTarget": "jz-portfolio:serve:production"
            }
          },
          "options": {
            "devServerTarget": "jz-portfolio:serve",
            "protractorConfig": "e2e/protractor.conf.js"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "jz-portfolio:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "jz-portfolio:build:production"
            }
          },
          "options": {
            "browserTarget": "jz-portfolio:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        },
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "src"
    }
  },
  "schematics": {
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    }
  }
}
